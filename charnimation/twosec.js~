function loadChars() {
timedText(); 
timedText2();
timedText3();
timedText4();
timedText5();
timedText6();
timedText7();
}
function timedText() {
  setTimeout(myTimeout1, 500) 
  setTimeout(myTimeout2, 1000) 
  setTimeout(myTimeout3, 1500) 
  setTimeout(myTimeout4, 2000)
}
function myTimeout1() {
var a= "\\O/"
var b= "&nbsp;" + "|"
var c= "/ \\"
var d= a + "<br>" + b + "<BR>" + c
  document.getElementById("demo").innerHTML = d;
}
