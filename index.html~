<script type="text/javascript" >
document.getElementById("intro").innerHTML = "<pre><span id='one'>      </span><img id='logo' height=290 width = 210 src='logo.jpg'><span id='two'>      </span></pre></span>"
var i = 0;
var j = 0;
var txt = ['      ', '     d', '    ed', '   ted', '  ited', ' nited', 'United'];
var txt2 = ['      ', 'e     ', 'ee    ', 'eek   ', 'eekd  ', 'eekdo ', 'eekdom'];
var speed = 100;

function typeWriter1() {
  if (i < txt.length) {
    document.getElementById("one").innerHTML = txt[i];
    i++;
    setTimeout(typeWriter1, speed);
  }
}
function typeWriter2() {
  if (j < txt2.length) {
    document.getElementById("two").innerHTML = txt2[j];
    j++;
    setTimeout(typeWriter2, speed);
  }
}
typeWriter1();
typeWriter2();
			function fadeOut(id,spd){
				if(id.style.opacity == ""){
					id.style.opacity = 1;
				}
				if(id.style.opacity > 0){
					var opac = 1;
					var cycle = setInterval(decreaseOpacity,spd);
					function decreaseOpacity() {
						opac -= 0.01;
						if(opac <= 0){
							id.style.opacity = 0;
							opac = 0;
							clearInterval(cycle);
							document.getElementById("intro").style.display = "none";
							document.getElementById("body").style.display = "block";
						}
						id.style.opacity = opac;
					}
				} else {
					clearInterval(cycle);
				}
			}
fadeOut(intro, 45)

</script>
